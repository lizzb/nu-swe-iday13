]

  function setupPivot(input){
    input.callbacks = {afterUpdateResults: function(){
      $('#results > table').dataTable({
        "sDom": "<'row'<'span6'l><'span6'f>>t<'row'<'span6'i><'span6'p>>",
        "iDisplayLength": -1,
        "aLengthMenu": [[-1, 15, 25, 50], ["All", 15, 25, 50]],
        "sPaginationType": "bootstrap",
        //"oLanguage": {
        //  "sLengthMenu": "_MENU_ records per page"
       // }
      });
    }};
    $('#pivot-demo').pivot_display('setup', input);
  };

  $(document).ready(function() {

    setupPivot({url:'./lib/csv/company-grid-and-locations.csv', fields: fields,
      rowLabels:[
          "Company",
          "Room  (Floor)", 
          "Applied Math","BME","Chem","Civil","Comp. Eng","Comp. Sci","EE",
          "Environ. Eng.","IE","MaDE","Mat. Sci","Mech. Eng","Non - Eng",
          "MS/PhD","Permanent","Co-op","Intern",
          ]
        }
        )
    //, filters: {company: 'Dropbox'},  summaries:["Permanent", "Co-op", "Intern"]})

    // prevent dropdown from closing after selection
    $('.stop-propagation').click(function(event){
      event.stopPropagation();
    });




  });


function fnShowHide( iCol )
{
    /* Get the DataTables object again - this is not a recreation, just a get of the object */
    var oTable = $('#results > table').dataTable();
     
    var bVis = oTable.fnSettings().aoColumns[iCol].bVisible;
    oTable.fnSetColumnVis( iCol, bVis ? false : true );
}

</script>


