// Define the structure of fields, if this is not defined then all fields will be assumed
// to be strings.  Name must match csv header row (which must exist) in order to parse correctly.
var fields = [
    // filterable fields
    {name: 'Company',     type: 'string', filterable: false, bAutoWidth: false }, 
    //, filterType: 'regexp'},
    {name: 'Room  (Floor)', type: 'string', filterable: true},
    {name: 'Table #',       type: 'string', filterable: true},

    {name: 'Applied Math',  type: 'string', filterable: true},
    {name: 'BME',           type: 'string', filterable: true},
    {name: 'Chem',          type: 'string', filterable: true},
    {name: 'Civil',         type: 'string', filterable: true},
    {name: 'Comp. Eng',     type: 'string', filterable: true},
    {name: 'Comp. Sci',     type: 'string', filterable: true},
    {name: 'EE',            type: 'string', filterable: true},
    {name: 'Environ. Eng.', type: 'string', filterable: true},
    {name: 'IE',            type: 'string', filterable: true},
    {name: 'MaDE',          type: 'string', filterable: true},
    {name: 'Mat. Sci',      type: 'string', filterable: true},
    {name: 'Mech. Eng',     type: 'string', filterable: true},
    {name: 'Non - Eng',     type: 'string', filterable: true},

    {name: 'MS/PhD',        type: 'string', filterable: true},
    
    {name: 'Intern',        type: 'string', filterable: true},
    {name: 'Co-op',         type: 'string', filterable: true},
    {name: 'Permanent',     type: 'string', filterable: true},
    
    

    /*
    // summary fields
    {name: 'billed_amount',     type: 'float',  rowLabelable: false, summarizable: 'sum', displayFunction: function(value){ return accounting.formatMoney(value)}},
    {name: 'payment_amount',    type: 'float',  rowLabelable: false, summarizable: 'sum', displayFunction: function(value){ return accounting.formatMoney(value)}},
    {name: 'balance', type: 'float', rowLabelable: false, pseudo: true,
      pseudoFunction: function(row){ return row.billed_amount - row.payment_amount },
      summarizable: 'sum', displayFunction: function(value){ return accounting.formatMoney(value)}},
    {name: 'last_payment_date',  type: 'date',  filterable: true}
